{
  "version": 2,
  "name": "agent4-multi-llm",
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/src/index.ts"
    },
    {
      "src": "/api/agent4/execute",
      "dest": "/src/index.ts",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": "7860",
    "HF_TOKEN": "@hf_token",
    "MISTRAL_API_KEY": "@mistral_api_key",
    "DEEPSEEK_API_KEY": "@deepseek_api_key",
    "OPENROUTER_API_KEY": "@openrouter_api_key",
    "CODESTRAL_API_KEY": "@codestral_api_key",
    "ALIBABA_QWEN_API_KEY": "@alibaba_qwen_api_key",
    "KIMI_API_KEY": "@kimi_api_key",
    "CONTINUE_API_KEY": "@continue_api_key"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "functions": {
    "src/index.ts": {
      "memory": 3008,
      "maxDuration": 60
    }
  }
}
